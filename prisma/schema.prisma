generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id    Int @id @default(autoincrement())
  name  String @db.VarChar(100)
  email String @unique
  password String 
  created_at DateTime @default(now())  
  role Role @default(USER)
  @@map("users")
}

enum Role{
  USER
  ADMIN
}  
